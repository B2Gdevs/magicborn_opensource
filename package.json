{
    "name": "magicborn_react",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "node scripts/dev.js",
        "build": "next build",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start -p 3000",
        "start:prod": "NODE_ENV=production next start -p 3000",
        "serve": "npm run build && npm run start",
        "serve:prod": "npm run build && npm run start:prod",
        "lint": "next lint",
        "test": "vitest",
        "test:run": "vitest run",
        "migrate:spells": "tsx scripts/migrate-spells-to-db.ts",
        "migrate:region-parents": "tsx scripts/migrate-region-parent-ids.ts",
        "payload:init": "tsx scripts/init-payload.ts",
        "payload:seed": "tsx scripts/seed-payload.ts",
        "payload:migrate-slugs": "tsx scripts/add-slug-columns.ts",
        "payload:add-page-number": "tsx scripts/add-page-number-column.ts",
        "payload:sync-version-slugs": "tsx scripts/sync-version-slug-columns.ts",
        "payload:init-pages": "tsx scripts/init-pages-table.ts",
        "payload:seed-prompts": "tsx scripts/seed-magicborn-prompts.ts",
        "postinstall": "patch-package",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:studio:payload": "npx drizzle-kit studio --config drizzle-payload.config.ts",
        "docker:up": "docker compose -f infra/ai-stack/docker-compose.yml up",
        "docker:up:detached": "docker compose -f infra/ai-stack/docker-compose.yml up -d",
        "docker:down": "docker compose -f infra/ai-stack/docker-compose.yml down",
        "docker:logs": "docker compose -f infra/ai-stack/docker-compose.yml logs -f",
        "docker:ps": "docker compose -f infra/ai-stack/docker-compose.yml ps",
        "docker:restart": "docker compose -f infra/ai-stack/docker-compose.yml restart"
    },
    "dependencies": {
        "@ai-sdk/openai": "^2.0.0",
        "@ai-sdk/openai-compatible": "2",
        "@ai-sdk/provider": "2",
        "@blocknote/core": "^0.45.0",
        "@blocknote/mantine": "^0.45.0",
        "@blocknote/react": "^0.45.0",
        "@blocknote/xl-ai": "^0.45.0",
        "@hookform/resolvers": "^5.2.2",
        "@mantine/core": "^8.3.10",
        "@mantine/hooks": "^8.3.10",
        "@payloadcms/db-sqlite": "^3.68.5",
        "@payloadcms/next": "^3.68.5",
        "@payloadcms/richtext-lexical": "^3.68.5",
        "@radix-ui/react-context-menu": "^2.2.16",
        "@tanstack/react-query": "^5.90.12",
        "@tanstack/react-virtual": "^3.13.13",
        "@tiptap/core": "^3.14.0",
        "@tiptap/extension-gapcursor": "^3.14.0",
        "@tiptap/extensions": "^3.14.0",
        "@tiptap/pm": "^3.14.0",
        "@tiptap/starter-kit": "^3.14.0",
        "@types/better-sqlite3": "^7.6.13",
        "ai": "5.0.116",
        "better-sqlite3": "^12.5.0",
        "clsx": "^2.1.1",
        "drizzle-kit": "^0.31.8",
        "drizzle-orm": "^0.45.0",
        "framer-motion": "^12.23.25",
        "graphql": "^16.10.0",
        "konva": "^9.2.3",
        "lucide-react": "^0.556.0",
        "next": "15.3.4",
        "payload": "^3.68.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.69.0",
        "react-hotkeys-hook": "^5.2.1",
        "react-icons": "^5.5.0",
        "react-konva": "^18.2.10",
        "react-markdown": "^10.1.0",
        "react-masonry-css": "^1.0.16",
        "react-player": "^3.4.0",
        "react-tooltip": "^5.30.0",
        "remark-gfm": "^4.0.1",
        "sonner": "^2.0.7",
        "swagger-ui-react": "^5.31.0",
        "tailwind-merge": "^3.4.0",
        "use-image": "^1.1.4",
        "vitest": "^4.0.14",
        "zod": "^4.2.1",
        "zustand": "^5.0.8"
    },
    "overrides": {
        "@ai-sdk/provider": "2"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^24.10.1",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/swagger-ui-react": "^5.18.0",
        "@vitejs/plugin-react": "^5.1.2",
        "ansi-to-html": "^0.7.2",
        "autoprefixer": "10.4.20",
        "detect-port": "^2.1.0",
        "eslint": "8.57.0",
        "eslint-config-next": "15.3.4",
        "get-port": "^7.1.0",
        "jsdom": "^27.3.0",
        "patch-package": "^8.0.1",
        "postcss": "8.4.49",
        "tailwindcss": "3.4.14",
        "tsx": "^4.21.0",
        "typescript": "5.6.3"
    }
}
